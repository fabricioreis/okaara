<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="okaara">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name="org.gwtbootstrap3.GwtBootstrap3" />
	<inherits name="org.fusesource.restygwt.RestyGWT" />
	<inherits name="org.gwtbootstrap3.extras.select.Select" />
	<inherits name="org.gwtbootstrap3.extras.growl.Growl" />
	<inherits name="org.gwtbootstrap3.extras.bootbox.Bootbox" />
	<inherits name="com.okaara.shared.Shared" />

	<extend-property name="locale" values="pt_BR" />
	<set-property name='locale' value='pt_BR' />
	<set-property-fallback name='locale' value='pt_BR' />
	
	<source path="client" />
	<entry-point class="com.okaara.client.OkaaraEntryPoint"></entry-point>
</module>